#This is the main function that allows the sequence to happen
def fib(n):

    a = 0
    b = 1

    if n == 1:
        print(a)

    print(a)
    print(b)

    for i in range(2, n):
        c = a + b
        a = b
        b = c
        print(c)

#Calling the function.
fib(45)

